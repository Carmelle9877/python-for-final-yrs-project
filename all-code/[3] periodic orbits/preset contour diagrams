# crazy plot
xco = [-80,3,1,0,0]
yco = xco
xyco = [80,5,20,5,1,1,0,0,0]

limits = -10,10,-10,10,5000

approx_xyp1 = [-3.7,-2.6,0.17] # [x,y,p]
