# cubic with x^2y terms
xco = [-80,3,1,0,0]
yco = [-80,3,1,0,0]
xyco = [0,3,-3,0,0,0,0,0,0]

limits = -12,10,-12,9,"none"

approx_xyp = [-0.7,-8.7,0.3] # [x,y,p]
E = -2000


# cubic with x^2y terms
xco = [-80,3,1,0,0]
yco = [-80,3,1,0,0]
